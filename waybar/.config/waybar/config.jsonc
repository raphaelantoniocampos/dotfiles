{
	"layer": "top",
	"position": "top",
	"height": 35,
	"spacing": 5,
	"margin-top": 3,
	"margin-right": 3,
	"margin-left": 3,
	"modules-left": [
		"sway/workspaces",
		"sway/window",
		"custom/paddc",
		"custom/paddc",
		"custom/paddc",
		"custom/media",
		"tray",
		"sway/mode"
	],
	"modules-center": [
		"clock#date",
		"clock#time",
	],
	"modules-right": [
		"battery",
		"cpu",
		"custom/paddc",
		"memory",
		"custom/paddc",
		"temperature",
		"custom/paddc",
		"backlight",
		"custom/paddc",
		"pulseaudio",
		"custom/paddc",
		"network",
		"custom/paddc",
		"bluetooth",
		"custom/paddc",
		"custom/zypper",
		"custom/paddc",
		"custom/weather",
		"custom/paddc",
		"custom/power",
		"custom/paddc",
		"custom/paddc",
	],
	//Modules configuration
	"sway/workspaces": {
		"disable-scroll": true,
		"all-outputs": true,
		"active-only": false,
		"warp-on-scroll": false,
		"format": "{icon}",
		"format-icons": {
			"1": "<span color='#89dceb' >Û∞¨∫</span>",
			"2": "<span color='#89dceb' >Û∞¨ª</span>",
			"3": "<span color='#89dceb' >Û∞¨º</span>",
			"4": "<span color='#89dceb' >Û∞¨Ω</span>",
			"5": "<span color='#89dceb' >Û∞¨æ</span>",
			"6": "<span color='#89dceb' >Û∞¨ø</span>",
			"7": "<span color='#89dceb' >Û∞≠Ä</span>",
			"8": "<span color='#89dceb' >Û∞≠Å</span>",
			"9": "<span color='#89dceb' >Û∞≠Ç</span>",
			"10": "<span color='#f38ba8' size='xx-large'>ÔÇÉ</span>",
			//"urgent": ": ÔÅ™",
			//"default": "",
			"focused": "<span color='#a6e3a1' size='xx-large'>Û∞Æî</span>",
		}
	},
	"custom/paddc": {
		"format": " ",
		"tooltip": false
	},
	"sway/window": {
		"format": "{app_id}",
		"max-length": 20
	},
	"custom/media": {
		"format": "{}",
		"tooltip": false,
		"interval": 1,
		"max-length": 35,
		"format-icons": {
			"spotify": "ÔÜº",
			"default": "üéú"
		},
		"escape": true,
		"exec": "$HOME/.config/waybar/custom_modules/mediaplayer.sh",
		"on-click-right": "playerctl next",
		"on-click-left": "playerctl previous",
		"on-click": "playerctl play-pause",
	},
	"tray": {
		"icon-size": 11,
		"spacing": 5
	},
	"sway/mode": {
		"format": "<span style=\"italic\">{}</span>"
	},
	"clock#date": {
		"format": "<span color='#f9e2af' >ÔÅ≥  </span>{:%e %b}",
		"tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
		"today-format": "<b>{}</b>",
		"on-click": "gnome-calendar",
		"min-length": 8,
		"max-length": 20
	},
	"clock#time": {
		"format": "<span color='#74c7ec' >ÔÄó  </span>{:%H:%M}",
		"tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
		"on-click": "gnome-calendar",
		"min-length": 8,
		"max-length": 12
	},
	"custom/weather": {
		"format": "<span>{}</span>",
		"exec": "curl 'https://wttr.in/?format=1'",
		"interval": 3600,
		"on-click": "gnome-weather"
	},
	"battery#bat2": {
		"bat": "BAT2"
	},
	"battery": {
		"interval": 1,
		"states": {
			"good": 80,
			"warning": 25,
			"critical": 10
		},
		"format-icons": [
			"ÔâÑ ",
			"ÔâÉ ",
			"ÔâÇ ",
			"ÔâÅ ",
			"ÔâÄ "
		],
		"format": "<span color='#a6e3a1' > {icon}</span>{capacity}%",
		"format-critical": "<span color='#f38ba8' > {icon}</span>{capacity}%",
		"format-warning": "<span color='#fab387' > {icon}</span>{capacity}%",
		"format-full": "<span color='#a6e3a1' > {icon}</span>{capacity}%",
		"format-charging": "<span color='#a6e3a1' > Ôá¶ </span>{capacity}%",
		"format-charging-warning": "<span color='#fab387' > Ôá¶ </span>{capacity}%",
		"format-charging-critical": "<span color='#f38ba8' > Ôá¶ </span>{capacity}%",
		"format-plugged": "<span color='#a6e3a1' > Ôá¶ </span>{capacity}%",
		"tooltip": false
	},
	"cpu": {
		"interval": "1",
		"format": "<span color='#f9e2af'> Ôíº </span>{usage}%",
		"on-click": "kitty -e htop --sort-key PERCENT_CPU",
		"tooltip": false
	},
	"memory": {
		"format": "<span color='#94e2d5'> ÔÉâ </span>{}%",
	},
	"temperature": {
		// "thermal-zone": 2,
		// "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
		"critical-threshold": 80,
		"format": "<span color='#f38ba8'> {icon}</span> {temperatureC}¬∞C",
		"format-critical": "{temperatureC}¬∞C {icon}",
		//"format": "{temperatureC}¬∞C {icon}",
		"format-icons": [
			"Ôù´",
			"Ôãâ",
			"Ôù©"
		]
	},
	"backlight": {
		// "device": "acpi_video1",
		"format": "<span color='#f9e2af'> {icon} </span> {percent}%",
		"format-icons": [
			"Óéç",
			"Óèì",
			"Óèë",
			"Óèè",
			"Óèé",
			"Óèç",
			"Óèä",
			"Óèà",
			"Óéõ"
		]
	},
	"pulseaudio": {
		"scroll-step": 3, // %, can be a float
		"format": "<span color='#cba6f7' > {icon} </span>{volume}%",
		"format-muted": "<span color='#cba6f7 > Óª® </span>{volume}%",
		"format-bluetooth": "<span color='#cba6f7 > Û∞Ç∞ </span>{volume}%",
		"format-bluetooth-muted": "<span color='#cba6f7' > Û∞Ç≤ </span>{volume}%",
		"format-source": "{volume}% ÔÑ∞",
		"on-click": "pavucontrol",
		"on-click-right": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
		"tooltip": true,
		"max-volume": 100,
		"format-icons": {
			"headphone": "ÔÄ•",
			"hands-free": "Ôñê",
			"headset": "Ôñê",
			"phone": "ÔÇï",
			"portable": "ÔÇï",
			"car": "ÔÜπ",
			"default": [
				"ÔÄ¶",
				"ÔÄß",
				"ÔÄ®"
			]
		},
	},
	"network": {
		// "interface": "wlp2*", // (Optional) To force the use of this interface
		"format-wifi": "<big>Ôá´</big>  {essid}",
		"format-ethernet": "Ôûñ {ipaddr}/{cidr}",
		"tooltip-format": "Ôûñ {ifname} via {gwaddr}",
		"format-linked": " Ôûñ {ifname} (No IP)",
		"format-disconnected": "‚ö† Disconnected",
		"format-alt": "{ifname}: {ipaddr}/{cidr}"
	},
	"bluetooth": {
		"format": "<span color='#74c7ec'>Û∞Ç∞ </span>",
		"format-disabled": "<span color='#89b4fa'>Û∞Ç≤ </span>",
		"format-connected": "<span color='#89dceb'>Û∞Ç± </span>",
		"format-connected-battery": "<span color='#89dceb'>Û∞Ç± </span>",
		"tooltip-format": "{num_connections} connected",
		"tooltip-format-disabled": "Bluetooth Disabled",
		"tooltip-format-connected": "{num_connections} connected\n{device_enumerate}",
		"tooltip-format-enumerate-connected": "{device_alias}",
		"tooltip-format-enumerate-connected-battery": "{device_alias}: {device_battery_percentage}%",
		"on-click": "~/.config/waybar/scripts/bluetooth-menu.sh",
		"interval": 1,
		"max-length": 10
	},
	"custom/zypper": {
		"format": "<span color='#a6e3a1'> Û∞ö∞ </span>{}%",
		"interval": 3600, // every hour
		"exec": "echo $(($(zypper lu | wc -l) - 3))", // # of updates
		"exec-if": "exit 0", // always run; consider advanced run conditions
		"on-click": "kitty -e sudo zypper refresh && sudo zypper dup --allow-vendor-change && pkill -SIGRTMIN+8 waybar", // update system
		"signal": 8,
		"max-length": 5,
		"min-length": 5
	},
	"custom/power": {
		"format": "‚èª ",
		"tooltip": false,
		"menu": "on-click",
		"menu-file": "$HOME/.config/waybar/custom_modules/power_menu.xml", // Menu file in resources folder
		"menu-actions": {
			"shutdown": "shutdown",
			"reboot": "reboot",
			"suspend": "systemctl suspend",
			"hibernate": "systemctl hibernate"
		}
	},
}
