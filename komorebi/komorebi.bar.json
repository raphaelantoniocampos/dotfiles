{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.39/schema.bar.json",
  "monitor": 0,
  "font_family": "0xProto Nerd Font Mono",
  "font_size": 10,
  "height": 30,
  "theme": {
    "palette": "Base16",
    "name": "Paraiso",
    "accent": "Base0D"
  },
  "transparency_alpha": 0,
  "padding": {
    "top": 20,
    "bottom": 20,
    "left": 30,
    "right": 30
  },
  "left_widgets": [
    {
      "Komorebi": {
        "focused_window": {
          "enable": true,
          "show_icon": true
        }
      }
    }
  ],
  "widget_spacing": 20,
  "center_widgets": [
    {
      "Komorebi": {
        "workspaces": {
          "enable": true,
          "hide_empty_workspaces": true
        }
      }
    }
  ],
  "right_widgets": []
}
